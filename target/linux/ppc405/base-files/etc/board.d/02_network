#!/bin/sh

. /lib/functions/system.sh
. /lib/functions/uci-defaults.sh

ppc405_setup_interfaces()
{
	local board="$1"

	case "$board" in
	    *)
		ucidef_set_interfaces_lan_wan "eth0" "eth1"
		;;
	esac
}

board_config_update
board=$(board_name)
ppc405_setup_interfaces $board
board_config_flush

exit 0
